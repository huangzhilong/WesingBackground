    <declare-styleable name="wesingBackground">
        
        <attr name="wb_shape" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="oval" value="1" />
            <enum name="line" value="2" />
            <enum name="ring" value="3" />
        </attr>

        <attr name="wb_solid_color" format="color" />

        <attr name="wb_corners_radius" format="dimension" />
        <attr name="wb_corners_bottomLeftRadius" format="dimension" />
        <attr name="wb_corners_bottomRightRadius" format="dimension" />
        <attr name="wb_corners_topLeftRadius" format="dimension" />
        <attr name="wb_corners_topRightRadius" format="dimension" />

        <attr name="wb_gradient_angle" format="integer" />
        <attr name="wb_gradient_centerX" format="float" />
        <attr name="wb_gradient_centerY" format="float" />
        <attr name="wb_gradient_centerColor" format="color" />
        <attr name="wb_gradient_endColor" format="color" />
        <attr name="wb_gradient_startColor" format="color" />
        <attr name="wb_gradient_gradientRadius" format="dimension" />

        <attr name="wb_gradient_type" format="enum">
            <enum name="linear" value="0" />
            <enum name="radial" value="1" />
            <enum name="sweep" value="2" />
        </attr>

        <attr name="wb_gradient_useLevel" format="boolean" />

        <attr name="wb_padding_left" format="dimension" />
        <attr name="wb_padding_top" format="dimension" />
        <attr name="wb_padding_right" format="dimension" />
        <attr name="wb_padding_bottom" format="dimension" />

        <attr name="wb_size_width" format="dimension">
            <enum name="wrap_content" value="-2" />
            <enum name="match_parent" value="-1" />
        </attr>
        <attr name="wb_size_height" format="dimension">
            <enum name="wrap_content" value="-2" />
            <enum name="match_parent" value="-1" />
        </attr>

        <attr name="wb_stroke_width" format="dimension" />
        <attr name="wb_stroke_color" format="color" />
        <attr name="wb_stroke_dashWidth" format="dimension" />
        <attr name="wb_stroke_dashGap" format="dimension" />
        
    </declare-styleable>
</resources>